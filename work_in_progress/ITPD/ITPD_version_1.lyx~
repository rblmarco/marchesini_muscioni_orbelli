framecolor "black"
backgroundcolor "none"
status collapsed

\begin_layout Plain Layout

\shape italic
To use the mobile application:
\end_layout

\begin_layout Itemize

\shape italic
A smartphone with:
\end_layout

\begin_deeper
\begin_layout Itemize

\shape italic
Android: KitKat (4.4) and later;
\end_layout

\begin_layout Itemize

\shape italic
iOS: version 6 or later;
\end_layout

\begin_layout Itemize

\shape italic
Windows Mobile: version 10 or later;
\end_layout

\end_deeper
\begin_layout Itemize

\shape italic
An active number, with a plans that contains internet connection:
\end_layout

\begin_deeper
\begin_layout Itemize

\shape italic
3G;
\end_layout

\begin_layout Itemize

\shape italic
4G: not required, but recommended.
 
\end_layout

\end_deeper
\begin_layout Itemize

\shape italic
An amount of space in order to install the application.
\end_layout

\end_inset


\end_layout

